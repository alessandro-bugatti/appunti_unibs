<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="crescente"/>
        <attribute name="authors" value="alex"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-10-06 04:55:14 "/>
        <attribute name="created" value="YWxleDtDQUxWSU47MjAyNS0xMC0wNjsiMDQ6MzU6MDggIjsyMDYy"/>
        <attribute name="edited" value="YWxleDtDQUxWSU47MjAyNS0xMC0wNjsiMDQ6NTU6MTQgIjsxOzIxNjk="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="N, numero, precedente, i" type="Integer" array="False" size=""/>
            <declare name="crescente" type="Boolean" array="False" size=""/>
            <assign variable="crescente" expression="true"/>
            <input variable="N"/>
            <input variable="precedente"/>
            <for variable="i" start="1" end="N-1" direction="inc" step="1">
                <input variable="numero"/>
                <if expression="precedente &gt; numero">
                    <then>
                        <assign variable="crescente" expression="false"/>
                    </then>
                    <else/>
                </if>
                <assign variable="precedente" expression="numero"/>
            </for>
            <if expression="crescente == true">
                <then>
                    <output expression="&quot;La sequenza &#232; crescente&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;La sequenza non &#232; crescente&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
